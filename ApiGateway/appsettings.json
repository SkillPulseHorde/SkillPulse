{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },


  "ReverseProxy": {
    "Routes": {
      "users": {
        "ClusterId": "usersCluster",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "usersCluster": {
        "Destinations": {
          "d1": { "Address": "http://userservice:8080" }
        }
      }
    }
  },
  
  
    "AllowedHosts": "*"
}
